{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "rootDir": "./src",
    "outDir": "./lib",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true
  },
  "exclude": [
    "test",
    "lib"
  ]
}
